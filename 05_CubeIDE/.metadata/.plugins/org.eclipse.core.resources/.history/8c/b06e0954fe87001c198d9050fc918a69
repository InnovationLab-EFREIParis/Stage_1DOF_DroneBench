#include "main.h"


void setGreenLed(){
	// Light up green led
	HAL_GPIO_WritePin(GPIOA, GPIO_PIN_5, SET);
}

void blinkGreenLed(){

	for (int i = 0; i < 5; i++) {
		HAL_GPIO_TogglePin(GPIOA, GPIO_PIN_5);
		HAL_Delay(100);
	}

	// Light up green led
	HAL_GPIO_WritePin(GPIOA, GPIO_PIN_5, SET);
}
