#include "yann.h"
#include "main.h"

uint16_t ADCinput(ADC_HandleTypeDef *hadc,int polTime){

	HAL_ADC_Start(&hadc);
	HAL_ADC_PollForConversion(&hadc1, polTime);
	return  HAL_ADC_GetValue(&hadc1);

}

void load_pwm(TIM_HandleTypeDef *htimX,uint16_t val){

	htimX.Instance->CCR1=val ;
}


